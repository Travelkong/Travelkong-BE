    "knex": "^3.1.0",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "nanoid": "^5.1.6",
    "nodemon": "^3.1.11",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
