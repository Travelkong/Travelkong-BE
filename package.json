{
  "name": "travelkong-be",
  "version": "1.0.0",
  "description": "Back-end code for the website Travelkong",
  "main": "index.ts",
  "scripts": {
    "build": "pnpx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "knex:cli": "NODE_OPTIONS='-r ts-node/register' knex --knexfile knexfile.js",
    "db:run": "pnpm knex migrate:latest && pnpm knex seed:run",
    "db:migrate": "pnpm run knex:cli migrate:latest",
    "db:rollback": "pnpm knex migrate:rollback",
    "db:seed": "pnpm knex seed:run",
    "db:migrate:status": "NODE_OPTIONS='-r ts-node/register' knex migrate:status --knexfile knexfile.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travelkong/Travelkong-BE.git"
  },
  "author": "Travelkong",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Travelkong/Travelkong-BE/issues"
  },
  "homepage": "https://github.com/Travelkong/Travelkong-BE#readme",
