    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "file-system": "^1.2.9",
    "firebase-admin": "^13.6.0",
    "http-errors": "^2.0.1",
